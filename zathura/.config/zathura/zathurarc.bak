### Settings from original zathurarc ###
set font "monospace normal 21"

### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#89bce4'
set completion-bg rgba(3,19,22,0.75)

# Command line completion group elements
set completion-group-fg '#B85D63'
set completion-group-bg rgba(3,19,22,0.75)

# Current command line completion element
set completion-highlight-fg '#031316'
set completion-highlight-bg '#89bce4'

# Default foreground/background color
set default-bg rgba(3,19,22,0.75)

# Input bar
set inputbar-fg '#89bce4'
set inputbar-bg rgba(3,19,22,0.75)

# Notification
set notification-fg '#89bce4'
set notification-bg rgba(3,19,22,0.75)

# Error notification
set notification-error-fg '#89bce4'
set notification-error-bg '#614C65'

# Warning notification
set notification-warning-fg '#89bce4'
set notification-warning-bg '#614C65'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#89bce4'
set statusbar-bg rgba(3,19,22,0.75)

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#B85D63'
set highlight-active-color '#B85D63'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#89bce4'

# 'Loading...' text
set render-loading-fg '#89bce4'
set render-loading-bg rgba(3,19,22,0.75)

# Index mode
set index-fg '#89bce4'
set index-bg rgba(3,19,22,0.75)

# Selected element in index mode
set index-active-fg '#031316'
set index-active-bg '#89bce4'